    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm start",
            "name": "Start dev server",
            "request": "launch",
            "type": "node-terminal",
            
        },
        {
            "name": "Attach to Edge",
            "port": 9222,
            "request": "attach",
            "type": "pwa-msedge",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "OctoPrint",
            "type": "python",
            "request": "launch",
            "module": "octoprint",
            "args": ["serve", "--debug"],
        }
    ]
}